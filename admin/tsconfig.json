{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "typeRoots": ["./node_modules/@types"],
    "lib": ["es2015", "dom"],
    "jsx": "react",
    "jsxFactory": "m"
  }
}
